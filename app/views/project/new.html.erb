<%= render :partial=> "common/admin_header"%>
 <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        <%= "#{t(:label_add)} #{t(:label_projects)}" %>
      </h1>
    </section>
    <% if not @project.errors.messages.nil? %>
    <%= @project.errors.messages.inspect %>
    <% end %>
    <!-- Main content -->
    <section class="content">
      <!-- Info boxes -->
      <div class="row">
        <div class=" col-xs-12">
          <div class="info-box">
            <div class="info-box-content">
              <%= form_for @project,url:project_index_path(@project),:html => {:class=>"form-horizontal"} do |l| %>
              <div class="box-body">
                <div class="form-group">
                  <%= l.label :name,:class=>"col-sm-2 control-label" %>

                  <div class="col-sm-6">
                    <%= l.text_field :name,:required=>true,:class=>"form-control" %>
                  </div>
                </div>
                <div class="form-group">
                  <%= l.label :project_type,:class=>"col-sm-2 control-label" %>

                  <div class="col-sm-6">
                    <%= l.text_field :project_type,:required=>true,:class=>"form-control" %>
                  </div>
                </div>
                <div class="form-group">
                  <%= l.label :project_code,:class=>"col-sm-2 control-label" %>

                  <div class="col-sm-6">
                    <%= l.text_field :project_code,:required=>true,:class=>"form-control" %>
                  </div>
                </div>
                <div class="form-group">
                  <%= l.label :start_date,:class=>"col-sm-2 control-label" %>

                  <div class="col-sm-6">
                    <%= l.date_field :start_date,:required=>true,:class=>"form-control" %>
                  </div>
                </div>
                <div class="form-group">
                  <%= l.label :end_date,:class=>"col-sm-2 control-label" %>

                  <div class="col-sm-6">
                    <%= l.date_field :end_date,:required=>true,:class=>"form-control" %>
                  </div>
                </div>
                <div class="form-group">
                  <%= l.label :sow_value,:class=>"col-sm-2 control-label" %>

                  <div class="col-sm-6">
                    <%= l.text_field :sow_value,:required=>true,:class=>"form-control" %>
                  </div>
                </div>
              </div>
              <!-- /.box-body -->
              <div class="box-footer">
                <%= l.submit :add,:class=>"btn btn-info" %>
              </div>
              <% end %>
            </div>
            <!-- /.info-box-content -->
          </div>
          <!-- /.info-box -->
        </div>
      </div>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->

  <footer class="main-footer">
    <div class="pull-right hidden-xs">
      <b>Version</b> 2.4.0
    </div>
    <strong>Copyright &copy; 2014-2016 <a href="https://adminlte.io">Almsaeed Studio</a>.</strong> All rights
    reserved.
  </footer>
</div>
<!-- ./wrapper -->
</body>
</html>