<%= form_for :users,:url=>create_user_path do |user| %>
<%= user.label :username %>
<%= user.text_field :username %>
<%= user.label :password %>
<%= user.password_field :password %>
<%= user.label :phone %>
<%= user.text_field :mobile %>
<%= user.label :address %>
<%= user.text_field :address %>
<%= user.label :email %>
<%= user.text_field :email %>
<%= user.submit :Register %>
<% end %>